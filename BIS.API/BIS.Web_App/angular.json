            "tsConfig": "projects/sharedlibrary/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "644d3e50-b369-4310-9fac-fb42880ae6e0"
  }
}
